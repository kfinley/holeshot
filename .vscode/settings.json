{
  "storyExplorer.storiesGlobs": "packages/*/src/stories/*.stories.ts",
  "dotnet-test-explorer.enableTelemetry": false,
  "dotnet-test-explorer.testProjectPath": "services/**/src/dotnet/tests/*Tests.csproj",
  "storyExplorer.server.internal.enabled": false,
  "storyExplorer.server.external.url": "http://localhost:6007",
  "jestrunner.debugOptions": {
    "skipFiles": ["<node_internals>/**"],
    "smartStep": true
  },
  "omnisharp.projectLoadTimeout": 600,
  "python.testing.pytestArgs": [
             "services/Crawler/src/python"
         ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true
}
